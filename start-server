from src.lib.server import Server
from src.lib.utils import check_server_args, parse_server_args

def main():
    print("Server started")
    args = parse_server_args()

    if not check_server_args(args):
        return

    print(args.host)
    server = Server(args.host, args.port, args)
    server.start()


if __name__ == "__main__":
    main()
